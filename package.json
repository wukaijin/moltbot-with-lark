{
  "name": "moltbot-with-lark",
  "version": "1.0.0",
  "description": "Bridge Lark (飞书) WebSocket connections to Moltbot agent with streaming responses",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "lark",
    "feishu",
    "moltbot",
    "ai",
    "websocket",
    "streaming"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/openai-compatible": "^1.0.0",
    "@larksuiteoapi/node-sdk": "^1.30.0",
    "ai": "^4.0.0",
    "dotenv": "^16.4.5",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
